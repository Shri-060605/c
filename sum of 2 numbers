#include <stdio.h>

int main() {
    int num1, num2, sum;
    
    // Get input from the user
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    
    // Calculate the sum
    sum = num1 + num2;
    
    // Display the result
    printf("The sum is: %d\n", sum);
    
    return 0;
}
